                </AnchorPane>
              </top>
            </BorderPane>
          </content>
          <graphic>
            <ImageView fitHeight="18.0" fitWidth="18.0" mouseTransparent="true" pickOnBounds="true" preserveRatio="false" smooth="true">
              <image>
